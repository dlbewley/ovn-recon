{
    "name": "ovn-recon",
    "version": "0.0.1",
    "private": true,
    "repository": "git@github.com:dlbewley/ovn-recon.git",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "npm run clean && NODE_ENV=production npm run ts-node node_modules/.bin/webpack",
        "build:dev": "npm run clean && NODE_ENV=development npm run ts-node node_modules/.bin/webpack",
        "start": "npm run ts-node node_modules/.bin/webpack serve",
        "start:dev": "npm run ts-node node_modules/.bin/webpack serve",
        "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
        "lint": "eslint ./src --fix",
        "test": "jest"
    },
    "consolePlugin": {
        "name": "ovn-recon",
        "version": "0.0.1",
        "displayName": "OVN Recon",
        "description": "OpenShift Console Plugin for OVN Recon",
        "exposedModules": {
            "NodeNetworkStateList": "./components/NodeNetworkStateList",
            "NodeNetworkStateDetails": "./components/NodeNetworkStateDetails"
        },
        "dependencies": {
            "@console/pluginAPI": "*"
        }
    },
    "dependencies": {
        "@patternfly/react-core": "^6.0.0",
        "@patternfly/react-icons": "^6.0.0",
        "@patternfly/react-table": "^6.0.0",
        "i18next": "^20.6.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "react-i18next": "^11.8.11",
        "react-router-dom": "^5.3.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@eslint/js": "^9.39.2",
        "@openshift-console/dynamic-plugin-sdk": "^1.0.0",
        "@openshift-console/dynamic-plugin-sdk-webpack": "^1.0.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^18.0.0",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-helmet": "^6.1.0",
        "@types/react-router-dom": "^5.3.2",
        "@types/webpack": "^5.28.0",
        "@types/webpack-dev-server": "^4.7.2",
        "@typescript-eslint/eslint-plugin": "^8.51.0",
        "@typescript-eslint/parser": "^8.51.0",
        "css-loader": "^6.5.1",
        "eslint": "^9.39.2",
        "eslint-plugin-react": "^7.37.5",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "style-loader": "^3.3.1",
        "ts-jest": "^29.4.6",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.1"
    }
}
